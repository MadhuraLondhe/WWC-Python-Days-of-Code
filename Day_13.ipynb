   "source": [
    "import random\n",
    "\n",
    "my_list = [\"Flower\", \"Fruit\", \"Leaf\", \"Trunk\"]\n",
    "random.shuffle(my_list)       # Using Shuffle to reorganize the order of the elements.\n",
    "\n",
    "print(my_list)"
   ]
